def res_fields:

  self.input = ""
  self.tokenInput = []
  self.subjectInput = []
  self.objectInput = []
  self.verb = ""
  if self.stack.isFull():
    val = self.stack.pop()
    self.stack.restet()
    self.stack.push(val)
  self.subjectResponse = ""
  self.output = ""
  self.personResponse = ""
  self.objectResponse = ""
  
      

 class Stack(object):

  """
    Create a object Stack whose 'tab' has a size initialized in the arguments of the constructor. 
    
    Eliza's memory
  
  """

  def __init__(self, size):
		self.tab = [self.size]
		self.cursor = 0

	def push(self, w):
		self.tab[self.cursor] = w
		self.cursor+=1

	def pop(self):
		w = self.tab[self.cursor]
		self.cursor -= 1
		return w

	def peek(self):
		return self.tab[self.cursor-1]

	def size(self):
		return self.cursor
    
  def resize(self):
    #how to? Or do we decide that the stack has an established size? 

#test OK
s = Stack(10)
s.push("lalala")
print s.peek()
print s.size()
      
      


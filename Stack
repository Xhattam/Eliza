 class Stack(object):

  """
    Create a object Stack whose 'tab' has a size initialized in the arguments of the constructor. 
    
    Eliza's memory
  
  """

  def __init__(self, size):
		self.tab = [self.size]
		self.cursor = 0

	def push(self, t):
		if len(t) > 1:
			if t[0] not in self.relative_lst:
				self.tab[self.cursor] = t
				self.cursor+=1
			else:
				t = (self.switchMe(t[0]), t[1])
				self.tab[self.cursor] = t
				self.cursor+=1
		else:
			pass
			
	def pop(self):
		w = self.tab[self.cursor]
		self.cursor -= 1
		return w

	def peek(self):
		return self.tab[self.cursor-1]

	def size(self):
		return self.cursor
    
  def resize(self):
    #how to? Or do we decide that the stack has an established size? 

#test OK
s = Stack(10)
s.push("lalala")
print s.peek()
print s.size()
      
      

